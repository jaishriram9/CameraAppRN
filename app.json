{
  "expo": {
    "name": "expo-camera-app",
    "slug": "expo-camera-app",
    "version": "1.1.0",
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "eas": {
        "projectId": "b6626ce9-0ebd-4dfa-93ed-01fa2efb753f"
      }
    },
    "android": {
      "config": {
        "keystoreAlias": "camera-app",
        "keystorePassword": "camera-app",
        "keystoreKeyPassword": "camera-app"
      }
    }
  },
  "plugins": [
    [
      "expo-media-library",
      {
        "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
        "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
        "isAccessMediaLocationEnabled": "true"
      }
    ]
  ]
}
